{
    "expo": {
        "name": "ShelterBridge",
        "slug": "ShelterBridge",
        "version": "1.0.19",
        "assetBundlePatterns": [
            "**/*"
        ],
        "icon": "./assets/images/icon.png",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "extra": {
            "eas": {
                "projectId": "66bc4a43-20e6-4175-8059-59f9fb2d15f6"
            }
        },
        "ios": {
            "bundleIdentifier": "com.shelterbridge.iosapp"
        },
        "plugins": [
            [
                "expo-location",
                {
                    "locationAlwaysAndWhenInUsePermission": "Allow ShelterBridge to use your location."
                }
            ]
        ],
        "android": {
            "permissions": [
                "android.permission.ACCESS_COARSE_LOCATION",
                "android.permission.ACCESS_FINE_LOCATION"
            ],
            "package": "com.shelterbridge.androidapp",
            "versionCode": 5,
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyBfW7FoEla3OsQ4VhdkWgD3wOfmCthN6Ag"
                }
            }
        }
    }
}